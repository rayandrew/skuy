{
  "name": "skuy",
  "version": "0.1.1",
  "description": "React SSR Power Pack",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": "https://github.com/rayandrews/skuy",
  "author": "Ray Andrew <raydreww@gmail.com>",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "files": [
    "dist",
    "core.js",
    "client.js",
    "document.js",
    "server.js"
  ],
  "keywords": [
    "react",
    "react-native",
    "react-native-web",
    "modal",
    "animation",
    "react-native-animatable"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:umd",
    "build:umd": "BABEL_ENV=commonjs babel src -d dist --ignore \"**/__tests__\"",
    "build:commonjs": "BABEL_ENV=commonjs babel src -d dist/cjs --ignore \"**/__tests__\"",
    "build:watch": "yarn build -w",
    "build:rollup": "yarn clean && rollup -c",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "next",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "hoist-non-react-statics": "^2.5.5",
    "prop-types": "^15.6.2",
    "react-helmet": "^5.2.0",
    "react-tree-walker": "^4.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^4.0.0",
    "serialize-javascript": "^1.5.0",
    "uglify-es": "^3.3.9",
    "webpack": "^4.15.1",
    "webpack-command": "^0.4.0"
  },
  "peerDependencies": {
    "react": "16.x.x",
    "react-art": "16.x.x",
    "react-dom": "16.x.x",
    "react-router-dom": "^4.x.x"
  }
}
